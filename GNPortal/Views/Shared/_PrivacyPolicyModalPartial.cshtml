
<div id="PrivacyPolicyModal" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header bg-gn-green">
                <h4 class="modal-title" style="color:white;">Privacy Policy</h4>
            </div>
            <div class="modal-body" style="max-height: 500px; overflow-y: scroll;">
                <div id="PrivacyPolicyContent"></div>
            </div>
            <div class="modal-footer text-center bg-gn-light-gray">
                <button id="PrivacyPolicyAcceptBtn" type="button" class="btn bg-gn-green white">
                    <span class="glyphicon glyphicon-ok"></span>
                    Accept
                </button>
            </div>
        </div>
    </div>
</div>

<div id="PrivacyPolicyContentSource" class="hide">
    @Html.Partial("../About/PrivacyPolicyContent")
</div>