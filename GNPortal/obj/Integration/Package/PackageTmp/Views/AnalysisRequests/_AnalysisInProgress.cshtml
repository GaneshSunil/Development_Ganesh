@using (Html.BeginGNPanel())
{
    //Heading
    using (Html.BeginGNPanelHeading("We are working on this analysis", "info-sign"))
    {

    }

    //Body
    using (Html.BeginGNPanelBody())
    {
        <div class="alert alert-success" role="alert">
